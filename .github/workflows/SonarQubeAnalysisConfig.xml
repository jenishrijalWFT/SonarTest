<?xml version="1.0" encoding="UTF-8"?>
<SonarQubeAnalysisConfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/SonarSource/sonar-scanner-msbuild/master/SonarQube.Integration.XmlSchema/Release/SonarQubeAnalysisConfig.xsd">
  <!-- General Configuration -->
  <SonarQubeServerUrl>http://localhost:9000</SonarQubeServerUrl>
  <ProjectKey>sonartest</ProjectKey>
  <ProjectName>sonartest</ProjectName>
  <ProjectVersion>1.0</ProjectVersion>

  <!-- Analysis Properties -->
  <SonarQubeHostedScannerProperties>
    <!-- Define the properties for the scanner -->
    <Property Name="sonar.sourceEncoding">UTF-8</Property>
    <Property Name="sonar.exclusions">**/*.txt</Property>
    <!-- Add more properties as per your project requirements -->
  </SonarQubeHostedScannerProperties>

  <!-- Advanced Configuration -->
  <AnalysisSettings>
    <!-- Configure additional analysis settings if needed -->
  </AnalysisSettings>
</SonarQubeAnalysisConfig>
